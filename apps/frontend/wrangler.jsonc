{
  "main": "dist/_worker.js/index.js",
  "name": "schastliv-frontend",
  "compatibility_date": "2026-01-20",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "schastliv.com",
      "custom_domain": true
    },
    {
      "pattern": "www.schastliv.com",
      "custom_domain": true
    }
  ]
}
